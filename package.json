{
  "name": "alfredos-renovations",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test:e2e": "pnpm supabase:start:test && playwright test",
    "supabase:start:test": "supabase start --workdir ./tests",
    "supabase:stop:test": "supabase stop --workdir ./tests",
    "supabase:start:dev": "supabase start",
    "supabase:stop:dev": "supabase stop",
    "supabase:diff": "pnpm supabase:start:dev && supabase db diff --file $(date +\"%Y%m%d_%H%M%S\")_migration --schema private,public",
    "supabase:gen:types": "pnpm supabase:start:dev && supabase gen types typescript --db-url \"postgresql://postgres:postgres@localhost:54322/postgres\" > ./src/features/db/supabase/types.ts",
    "supabase:push:dev": "pnpm supabase:start:dev && supabase db push --db-url \"postgresql://postgres:postgres@localhost:54322/postgres\"",
    "supabase:reset:test": "pnpm supabase:start:test && ./supabase/scripts/copy-migrations-to-test.sh && supabase db reset --workdir ./tests",
    "prisma:pull": "dotenv -e .env.development.local prisma db pull",
    "prisma:generate:dev": "dotenv -e .env.development.local prisma generate",
    "prisma:migrate": "dotenv -e .env.development.local prisma migrate dev",
    "prisma:generate": "prisma generate"
  },
  "dependencies": {
    "@bprogress/next": "^3.2.12",
    "@mantine/core": "^7.17.8",
    "@mantine/dates": "^8.1.3",
    "@mantine/dropzone": "^7.17.8",
    "@mantine/form": "^7.17.8",
    "@mantine/hooks": "^7.17.8",
    "@mantine/notifications": "^7.17.8",
    "@prisma/client": "^6.11.1",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.50.4",
    "@tabler/icons-react": "^3.34.0",
    "compressorjs": "^1.2.1",
    "firebase": "^11.10.0",
    "firebase-admin": "^13.4.0",
    "heic-convert": "^2.1.0",
    "next": "15.3.1",
    "next-intl": "^4.3.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "server-only": "^0.0.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@playwright/test": "^1.53.2",
    "@types/heic-convert": "^2.1.0",
    "@types/node": "^20.19.6",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "dotenv": "^16.6.1",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.30.1",
    "eslint-config-next": "15.3.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "postcss": "^8.5.6",
    "postcss-preset-mantine": "^1.18.0",
    "postcss-simple-vars": "^7.0.1",
    "prisma": "^6.11.1",
    "supabase": "^2.30.4",
    "typescript": "^5.8.3"
  }
}
